    "outDir": "dist",
    "moduleResolution": "node",
    "module": "commonjs",
    "resolveJsonModule": true,
    "target": "es6",
    "lib": ["es2017"],
    "sourceMap": true,
    "declaration": true
  },
  "include": [
	"src/**/*"
  ],
  "exclude": [
	"node_modules",
	"**/*.spec.ts"
  ]
}
